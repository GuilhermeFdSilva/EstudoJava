package fundamentos;

public class Wrapper {

	public static void main(String[] args) {
		Byte b = 100;
		Short s = 1000;
		Integer i = 10000;
		Long l = 100000L;
		
		System.out.println(b.doubleValue());
		System.out.println(s.toString());
		System.out.println(i.intValue());
		System.out.println(l.floatValue());
	}
}
